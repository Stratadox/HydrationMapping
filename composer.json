{
    "name": "stratadox/hydration-mapping",
    "license": "MIT",
    "authors": [
        {
            "name": "Stratadox",
            "homepage": "http://www.stratadox.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Stratadox\\Hydration\\Mapping\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stratadox\\HydrationMapping\\Test\\": "tests"
        }
    },
    "require": {
        "php": ">=7.1",
        "stratadox/hydration-mapping-contracts": "^0.1.3",
        "stratadox/hydrator-contracts": "^0.2",
        "stratadox/immutable-collection": "^1.1",
        "stratadox/proxy-contracts": "^0.1.0",
        "stratadox/specification-interfaces": "^1.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.9.2",
        "phpunit/phpunit": "^7.0",
        "satooshi/php-coveralls": "^2.0",
        "stratadox/hydrator": "^3.0",
        "stratadox/specification": "^1.0"
    },
    "suggest": {
        "stratadox/hydrator": "For hydrating the objects",
        "stratadox/proxy": "For lazy loading relationships",
        "stratadox/specification": "For advanced input validation"
    }
}
