{
    "name": "stratadox/hydration-mapping",
    "license": "MIT",
    "authors": [
        {
            "name": "Stratadox",
            "homepage": "http://www.stratadox.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Stratadox\\Hydration\\Mapping\\": "src"
        }
